<div class="session">
  {{#if session.isAuthenticated}}
    <div>
      {{#link-to 'profile'}}{{session.account}}{{/link-to}}
      <button {{action "logout"}}>Logout</button>
    </div>
  {{else}}
    {{#link-to "login"}}Login{{/link-to}} or
    {{#link-to "register"}}Register{{/link-to}}
  {{/if}}
</div>

{{outlet}}
